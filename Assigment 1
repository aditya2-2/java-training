package JavaOperators;
public class ArithmeticOperators 
{
	public static void main(String[]args)
	{
	int a=20;
	int b=10;
	 int p = a+b;
	 int q=a-b;
	int  r=a*b;
	int  s=a/b;
	float t=a%b;
	System.out.println(p);
	 System.out.println(q);
	 System.out.println(r);
	 System.out.println(s);
	 System.out.println(t);
     }
}
